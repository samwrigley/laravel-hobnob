{
    "name": "samwrigley/laravel-hobnob",
    "description": "A package for easily creating social links in Laravel applications.",
    "homepage": "https://github.com/samwrigley/laravel-hobnob",
    "authors": [
        {
            "name": "Sam Wrigley",
            "email": "sam@samwrigley.co.uk",
            "homepage": "https://samwrigley.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^5.4|^5.5|^5.6",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "SamWrigley\\Hobnob\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SamWrigley\\Hobnob\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SamWrigley\\Hobnob\\HobnobServiceProvider"
            ],
            "aliases": {
                "Handle": "SamWrigley\\Hobnob\\Facades\\SocialNetworkFacade"
            }
        }
    }
}
